  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "babel-preset-gatsby-package": "^0.1.3",
    "cross-env": "^5.2.0",
    "np": "^4.0.2"
  }
